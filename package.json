{
  "name": "react-static-template",
  "version": "1.0.0",
  "description": "Template for React",
  "scripts": {
    "build:min": "cross-env NODE_ENV=production TIMESTAMP=true webpack --progress --hide-modules --config config/webpack.prod.config.js",
    "build": "cross-env NODE_ENV=production BEAUTIFY=true TIMESTAMP=true webpack --progress --hide-modules --config config/webpack.prod.config.js",
    "prod": "cross-env NODE_ENV=production SOURCEMAP=true nodemon -e js --watch config --watch server server/server.config.js",
    "start": "cross-env NODE_ENV=development nodemon -e js --watch config --watch server server/server.config.js",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage --config config/jest.config.js",
    "test": "cross-env NODE_ENV=test jest --watchAll --config config/jest.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:SE7ENSKY/react-static-template.git"
  },
  "author": "se7ensky",
  "license": "ISC",
