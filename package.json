{
  "name": "react-static-template",
  "version": "1.0.0",
  "description": "Template for React",
  "scripts": {
    "build:min": "cross-env NODE_ENV=production TIMESTAMP=true webpack --config configs/webpack.prod.config.js",
    "build": "cross-env NODE_ENV=production BEAUTIFY=true TIMESTAMP=true webpack --config configs/webpack.prod.config.js",
    "prod": "cross-env NODE_ENV=production SOURCEMAP=true nodemon -e js --watch configs --watch server server/server.config.js",
    "start": "cross-env NODE_ENV=development nodemon -e js --watch configs --watch server server/server.config.js",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage",
    "test": "cross-env NODE_ENV=test jest --watchAll"
  },
  "engines": {
    "node": ">=8.11.1",
    "npm": ">=5.6.0",
    "yarn": ">=1.6.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:SE7ENSKY/react-static-template.git"
  },
  "author": "se7ensky",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^9.0.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^23.4.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-date-fns": "^0.2.0",
    "babel-plugin-dynamic-import-webpack": "^1.0.2",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-styled-components": "^1.5.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators": "^6.24.1",
    "babel-plugin-transform-imports": "^1.5.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.13",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "circular-dependency-plugin": "^5.0.2",
    "clean-webpack-plugin": "^0.1.19",
    "compression": "^1.7.3",
    "console-stamp": "^0.2.6",
    "copy-webpack-plugin": "^4.5.2",
    "cors": "^2.8.4",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "css-mqpacker": "^7.0.0",
    "cssnano": "^4.0.3",
    "eslint": "^5.1.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "event-source-polyfill": "^0.0.12",
    "express": "^4.16.3",
    "extract-text-webpack-plugin": "next",
    "file-loader": "^1.1.11",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "imports-loader": "^0.8.0",
    "lodash": "^4.17.10",
    "modernizr-webpack-plugin": "^1.0.7",
    "node-sass": "^4.9.2",
    "nodemon": "^1.18.3",
    "offline-plugin": "^5.0.5",
    "perfectionist": "^2.4.0",
    "postcss": "^7.0.0",
    "postcss-loader": "^2.1.6",
    "progress-bar-webpack-plugin": "^1.11.0",
    "react-perf-devtool": "^3.0.8",
    "sass-loader": "^7.0.3",
    "script-ext-html-webpack-plugin": "^2.0.1",
    "style-loader": "^0.21.0",
    "styles-postprocessor-plugin": "^1.1.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "verstat-nib": "^1.1.1",
    "webpack": "^4.16.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-middleware": "^2.22.3",
    "webpack-merge": "^4.1.3",
    "webpack-visualizer-plugin": "^0.1.11"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "date-fns": "^1.29.0",
    "element-closest": "^2.0.2",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "formik": "^1.0.1",
    "google-libphonenumber": "^3.1.10",
    "history": "^4.7.2",
    "immutable": "^4.0.0-rc.9",
    "intersection-observer": "^0.5.0",
    "jest": "^23.4.1",
    "normalize.css": "^8.0.0",
    "normalizr": "^3.2.4",
    "parse-link-header": "^1.0.1",
    "prop-types": "^15.6.2",
    "query-string": "^6.1.0",
    "raf": "^3.4.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-helmet": "^5.2.0",
    "react-lazyload": "^2.3.0",
    "react-loadable": "^5.4.0",
    "react-modal": "^3.5.1",
    "react-number-format": "^3.5.0",
    "react-paginate": "^5.2.3",
    "react-perf-devtool": "^3.0.8",
    "react-redux": "^5.0.7",
    "react-responsive": "^5.0.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "next",
    "react-router-transition": "^1.2.1",
    "react-select": "^1.2.1",
    "react-share": "^2.2.0",
    "react-spinkit": "^3.0.0",
    "react-spring": "^5.4.0",
    "react-tabs": "^2.2.2",
    "react-test-render": "^1.1.1",
    "react-tooltip": "^3.6.1",
    "react-virtualized": "^9.20.1",
    "recompose": "^0.27.1",
    "redux": "^4.0.0",
    "redux-api-middleware": "^2.3.0",
    "redux-define": "^1.1.1",
    "redux-thunk": "^2.3.0",
    "reselect": "^3.0.1",
    "sass-resources-loader": "^1.3.3",
    "shave": "^2.2.2",
    "smoothscroll-polyfill": "^0.4.3",
    "uuid": "^3.3.2",
    "whatwg-fetch": "^2.0.4",
    "yup": "^0.25.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}",
      "!**/node_modules/**",
      "!**/vendor/**"
    ],
    "setupFiles": [
      "./configs/jest.setup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "coverageDirectory": "<rootDir>/coverage",
    "globals": {
      "__DEV__": true
    },
    "verbose": true,
    "testMatch": [
      "**/__tests__/**/*.{js,jsx,mjs}",
      "**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.(css|less|styl|scss|sass|sss)$": "<rootDir>/configs/csstransform.config.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/configs/filetransform.config.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ],
    "moduleNameMapper": {
      "^components(.*)$": "<rootDir>/src/components$1",
      "^containers(.*)$": "<rootDir>/src/containers$1",
      "^layouts(.*)$": "<rootDir>/src/layouts$1",
      "^reducers(.*)$": "<rootDir>/src/reducers$1",
      "^routes(.*)$": "<rootDir>/src/routes$1",
      "^static(.*)$": "<rootDir>/src/static$1",
      "^f[/](.*)$": "<rootDir>/src/static/f$1",
      "^i[/](.*)$": "<rootDir>/src/static/i$1",
      "^v[/](.*)$": "<rootDir>/src/static/v$1",
      "^store(.*)$": "<rootDir>/src/store$1",
      "^styles(.*)$": "<rootDir>/src/styles$1",
      "^utils(.*)$": "<rootDir>/src/utils$1",
      "^vendor(.*)$": "<rootDir>/src/vendor$1"
    }
  },
  "babel": {
    "env": {
      "test": {
        "plugins": [
          "transform-decorators",
          "syntax-dynamic-import",
          "dynamic-import-webpack",
          "transform-class-properties",
          "transform-object-rest-spread"
        ],
        "presets": [
          "react",
          [
            "env"
          ]
        ]
      },
      "production": {
        "plugins": [
          "lodash",
          "date-fns",
          "transform-react-remove-prop-types",
          "transform-imports",
          "styled-components",
          "transform-decorators",
          "syntax-dynamic-import",
          "dynamic-import-webpack",
          "transform-class-properties",
          "transform-object-rest-spread",
          "transform-react-constant-elements",
          "transform-react-inline-elements"
        ],
        "presets": [
          "react",
          [
            "env",
            {
              "targets": {
                "browsers": [
                  ">0.25%",
                  "not op_mini all"
                ]
              },
              "modules": false,
              "loose": false,
              "spec": false,
              "useBuiltIns": true
            }
          ]
        ]
      },
      "development": {
        "plugins": [
          "lodash",
          "date-fns",
          "transform-decorators",
          "syntax-dynamic-import",
          "dynamic-import-webpack",
          "transform-class-properties",
          "transform-object-rest-spread"
        ],
        "presets": [
          "react",
          [
            "env",
            {
              "targets": {
                "browsers": [
                  ">0.25%",
                  "not op_mini all"
                ]
              },
              "modules": false,
              "loose": false,
              "spec": false,
              "useBuiltIns": true
            }
          ]
        ]
      }
    }
  }
}
