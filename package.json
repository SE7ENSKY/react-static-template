{
  "name": "react-static-template",
  "version": "1.0.0",
  "description": "Template for React",
  "scripts": {
    "build:min": "cross-env NODE_ENV=production TIMESTAMP=true webpack --config configs/webpack.prod.config.js",
    "build": "cross-env NODE_ENV=production BEAUTIFY=true TIMESTAMP=true webpack --config configs/webpack.prod.config.js",
    "prod": "cross-env NODE_ENV=production SOURCEMAP=true nodemon -e js --watch configs --watch server server/server.config.js",
    "start": "cross-env NODE_ENV=development nodemon -e js --watch configs --watch server server/server.config.js",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage",
    "test": "cross-env NODE_ENV=test jest --watchAll"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:SE7ENSKY/react-static-template.git"
  },
  "author": "se7ensky",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-date-fns": "^0.1.0",
    "babel-plugin-dynamic-import-webpack": "^1.0.2",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-styled-components": "^1.5.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators": "^6.24.1",
    "babel-plugin-transform-imports": "^1.5.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.13",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "circular-dependency-plugin": "^5.0.2",
    "clean-webpack-plugin": "^0.1.19",
    "compression": "^1.7.2",
    "console-stamp": "^0.2.6",
    "copy-webpack-plugin": "^4.5.1",
    "cors": "^2.8.4",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "css-mqpacker": "^6.0.2",
    "cssnano": "^3.10.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "event-source-polyfill": "^0.0.12",
    "express": "^4.16.3",
    "extract-text-webpack-plugin": "next",
    "file-loader": "^1.1.11",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "imports-loader": "^0.8.0",
    "lodash": "^4.17.5",
    "modernizr-webpack-plugin": "^1.0.7",
    "node-sass": "^4.8.3",
    "nodemon": "^1.17.3",
    "offline-plugin": "^4.9.0",
    "perfectionist": "^2.4.0",
    "postcss": "^6.0.21",
    "postcss-loader": "^2.1.4",
    "progress-bar-webpack-plugin": "^1.11.0",
    "sass-loader": "^7.0.1",
    "script-ext-html-webpack-plugin": "^2.0.1",
    "style-loader": "^0.21.0",
    "styles-postprocessor-plugin": "^1.1.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "verstat-nib": "^1.1.1",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.14",
    "webpack-dev-middleware": "^3.1.2",
    "webpack-hot-middleware": "^2.22.1",
    "webpack-merge": "^4.1.2",
    "webpack-visualizer-plugin": "^0.1.11"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "classnames": "^2.2.5",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.3",
    "formik": "^0.11.11",
    "history": "^4.7.2",
    "immutable": "^4.0.0-rc.9",
    "jest": "^22.4.3",
    "normalize.css": "^8.0.0",
    "normalizr": "^3.2.4",
    "prop-types": "^15.6.1",
    "raf": "^3.4.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-helmet": "^5.2.0",
    "react-lazyload": "^2.3.0",
    "react-loadable": "^5.3.1",
    "react-modal": "^3.4.2",
    "react-number-format": "^3.3.2",
    "react-paginate": "^5.2.2",
    "react-redux": "^5.0.7",
    "react-responsive": "^4.1.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "next",
    "react-select": "^1.2.1",
    "react-share": "^2.1.1",
    "react-spring": "^4.2.1",
    "react-tabs": "^2.2.2",
    "react-test-render": "^1.1.1",
    "react-tooltip": "^3.5.0",
    "react-virtualized": "^9.18.5",
    "redux": "^4.0.0",
    "redux-api-middleware": "^2.3.0",
    "redux-define": "^1.1.1",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "shave": "^2.1.3",
    "uuid": "^3.2.1",
    "whatwg-fetch": "^2.0.4",
    "yup": "^0.24.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}",
      "!**/node_modules/**",
      "!**/vendor/**"
    ],
    "coverageDirectory": "<rootDir>/coverage",
    "globals": {
      "__DEV__": true
    },
    "verbose": true,
    "testMatch": [
      "**/__tests__/**/*.{js,jsx,mjs}",
      "**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.(css|less|styl|scss|sass|sss)$": "<rootDir>/configs/csstransform.config.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/configs/filetransform.config.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ],
    "moduleNameMapper": {
      "^components(.*)$": "<rootDir>/src/components$1",
      "^containers(.*)$": "<rootDir>/src/containers$1",
      "^layouts(.*)$": "<rootDir>/src/layouts$1",
      "^reducers(.*)$": "<rootDir>/src/reducers$1",
      "^routes(.*)$": "<rootDir>/src/routes$1",
      "^static(.*)$": "<rootDir>/src/static$1",
      "^f[/](.*)$": "<rootDir>/src/static/f$1",
      "^i[/](.*)$": "<rootDir>/src/static/i$1",
      "^v[/](.*)$": "<rootDir>/src/static/v$1",
      "^store(.*)$": "<rootDir>/src/store$1",
      "^styles(.*)$": "<rootDir>/src/styles$1",
      "^utils(.*)$": "<rootDir>/src/utils$1",
      "^vendor(.*)$": "<rootDir>/src/vendor$1"
    }
  },
  "babel": {
    "plugins": [
      "lodash",
      "date-fns",
      "transform-react-remove-prop-types",
      "transform-imports",
      "babel-plugin-styled-components",
      "transform-decorators",
      "syntax-dynamic-import",
      "dynamic-import-webpack",
      "transform-class-properties",
      "transform-object-rest-spread"
    ],
    "presets": [
      "react",
      [
        "env",
        {
          "targets": {
            "browsers": [
              "last 3 versions",
              "Explorer >= 11",
              "Safari >= 9"
            ]
          },
          "modules": false,
          "useBuiltIns": true
        }
      ]
    ],
    "env": {
      "test": {
        "plugins": [
          "transform-decorators",
          "syntax-dynamic-import",
          "dynamic-import-webpack",
          "transform-class-properties",
          "transform-object-rest-spread"
        ],
        "presets": [
          "react",
          [
            "env"
          ]
        ]
      }
    }
  }
}
